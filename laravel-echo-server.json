{
  "authHost": "https://retail.cloudapp.my.id",
  "authEndpoint": "/broadcasting/auth",
  "clients": [],
  "database": "redis",
  "databaseConfig": {
    "redis": {
      "host": "127.0.0.1",
      "port": "6379",
      "password": "",
      "keyPrefix": "retail-database-"
    }
  },
  "devMode": true,
  "host": "0.0.0.0",
  "port": "9999",
  "protocol": "http",
  "socketio": {
    "cors": {
      "origin": "*",
      "methods": ["GET", "POST"],
      "credentials": true,
      "allowEIO3": true
    },
    "transports": ["websocket", "polling"],
    "pingInterval": 25000,
    "pingTimeout": 60000
  },
  "secureOptions": 67108864,
  "sslCertPath": "",
  "sslKeyPath": "",
  "sslCertChainPath": "",
  "sslPassphrase": "",
  "subscribers": {
    "http": true,
    "redis": true,
    "log": true
  },
  "apiOriginAllow": {
    "allowCors": true,
    "allowOrigin": "*",
    "allowMethods": "GET, POST, PUT, PATCH, OPTIONS",
    "allowHeaders": "Origin, Content-Type, X-Auth-Token, X-Requested-With, Accept, Authorization, X-CSRF-TOKEN, X-Socket-Id"
  }
}
